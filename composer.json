{
    "name": "2515104337/hyperf-eloquent-filter",
    "description": "An Eloquent way to filter Eloquent Models for Hyperf framework",
    "keywords": [
        "hyperf",
        "eloquent",
        "model",
        "search",
        "query",
        "filter"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alnico",
            "email": "2515104337@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "hyperf/database": "~3.0|~3.1",
        "hyperf/command": "~3.0|~3.1",
        "hyperf/config": "~3.0|~3.1",
        "hyperf/stringable": "~3.0|~3.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "HyperfEloquentFilter\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "HyperfEloquentFilter\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
